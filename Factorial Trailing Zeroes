// https://leetcode.com/problems/factorial-trailing-zeroes/

class Solution {
public:
    int trailingZeroes(int n) {
	int j;
	int count5 = 0;
	int count = 0;
	for (long long i = 5; n / i >= 1; i *= 5){
		count5 = count5 + n / i;           
		}                                  
	return count5;
    }
};
